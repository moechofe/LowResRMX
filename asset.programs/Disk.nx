U=X-TOUCH.X
V=Y-TOUCH.Y
SCROLL 3,OX+U,OY+V
END IF

BG 0
PAL 1
TEXT 1,1,STR$(SCROLL.X(3))+"X"+STR$(SCROLL.Y(3))+"      "

WAIT VBL

LOOP



